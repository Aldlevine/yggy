{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src/comm/__init__.ts",
        "src/jsx/__init__.ts",
        "src/model/__init__.ts",
        "src/observable/__init__.ts",
        "src/utils/__init__.ts"
    ],
    "categorizeByGroup": true,
    "skipErrorChecking": true,
    "jsDocCompatibility": true,
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "out": "./docs/ts"
}